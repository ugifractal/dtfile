<h1>New category</h1>

<% form_for(@data_file, :data_file, {:url => "/dtfile/admin/manage_files", :html => {:multipart => true}}) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= file_field :data_file, :file %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', "/dtfile/admin/manage_files" %>
